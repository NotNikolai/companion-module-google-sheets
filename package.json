{
	"name": "google-sheets",
	"version": "1.0.1",
	"main": "dist/index.js",
	"scripts": {
		"dev": "yarn build:watch",
		"build": "rimraf dist && yarn build:main",
		"build:main": "tsc -p tsconfig.build.json",
		"build:watch": "tsc -p tsconfig.build.json --watch",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"test": "jest"
	},
	"contributors": [],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bitfocus/companion-module-google-sheets.git"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,json,md}": [
			"prettier --write",
			"git add"
		],
		"*.{ts}": [
			"yarn lint --fix --",
			"git add"
		]
	},
	"eslintIgnore": [
		"/tests/*.test.ts"
	],
	"license": "MIT",
	"dependencies": {
		"@companion-module/base": "~1.1.0",
		"got-cjs": "12.5.4",
		"open": "~8.4.0"
	},
	"devDependencies": {
		"@companion-module/tools": "~0.5.2",
		"@types/jest": "~27.4.0",
		"@types/node": "~17.0.12",
		"@typescript-eslint/eslint-plugin": "~5.10.1",
		"@typescript-eslint/parser": "~5.10.1",
		"eslint": "~8.28.0",
		"eslint-config-prettier": "~8.5.0",
		"eslint-plugin-prettier": "~4.2.1",
		"husky": "~8.0.2",
		"jest": "~29.3.1",
		"lint-staged": "~13.0.4",
		"prettier": "~2.8.0",
		"rimraf": "~3.0.2",
		"ts-jest": "~29.0.3",
		"typescript": "~4.9.3"
	}
}
